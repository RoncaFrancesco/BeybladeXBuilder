    <!-- Decorative elements -->
    <circle cx="-50" cy="-50" r="8" fill="white" opacity="0.7" />
    <circle cx="50" cy="-50" r="8" fill="white" opacity="0.7" />
    <circle cx="-50" cy="50" r="8" fill="white" opacity="0.7" />
    <circle cx="50" cy="50" r="8" fill="white" opacity="0.7" />
  </g>

  <!-- Text at bottom -->
  <text x="96" y="165" font-family="Arial, sans-serif" font-size="18" font-weight="bold" text-anchor="middle" fill="white">
    BEYBLADE X
